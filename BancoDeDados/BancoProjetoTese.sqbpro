lue="127"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">CREATE TABLE Reagentes (
	Id INT(11) PRIMARY KEY ,  
    Nome VARCHAR(100) NOT NULL,        
    Formula VARCHAR(50),                 
    CAS VARCHAR(20),                     
    Unidade VARCHAR(20)                  
);

CREATE TABLE Localizacao (
    Id INT,                              -- vai referenciar o reagente
    Posicao VARCHAR(50),
    Prateleira VARCHAR(50),
    Armario VARCHAR(50),
    Quantidade DECIMAL(10,2),            -- quantidade com casas decimais
    PRIMARY KEY (Id),                    -- chave primária também é o Id
    FOREIGN KEY (Id) REFERENCES Reagentes(Id) -- chave estrangeira ligando ao reagente
);
